{% extends 'base_back.html.twig' %}

{% block title %}Jeux{% endblock %}

{% block body %}

    <h1>Jeux</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ jeux.id }}</td>
            </tr>
            <tr>
                <th>Type</th>
                <td>{{ jeux.type }}</td>
            </tr>
            <tr>
                <th>Titre</th>
                <td>{{ jeux.titre }}</td>
            </tr>
            <tr>
                <th>Image</th>
                <td>{{ jeux.image }}</td>
            </tr>
            <tr>
                <th>Date_debut</th>
                <td>{{ jeux.dateDebut ? jeux.dateDebut|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Date_fin</th>
                <td>{{ jeux.dateFin ? jeux.dateFin|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Produit</th>
                <td>{{ jeux.produit }}</td>
            </tr>
            <tr>
                <th>Prix</th>
                <td>{{ jeux.prix }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_jeux_index') }}">back to list</a>

    <a href="{{ path('app_jeux_edit', {'id': jeux.id}) }}">edit</a>

    {{ include('jeux/_delete_form.html.twig') }}
{% endblock %}
