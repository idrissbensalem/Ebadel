{% extends './base_front.html.twig' %} {% block title %}edit password{% endblock
%} {% block body %}

<section class="signup-area ptb-100">
  <div class="container">
    <div class="signup-content">
      <h2>Edit your Account</h2>

      {{ form_start(registrationForm, {'attr': {'class': 'signup-form'}}) }}
      {{ form_errors(registrationForm) }}
      <div class="form-group">
        
          {{
            form_row(registrationForm.plainPassword, {
              label: "Password"
            })
          }}
        
      </div>
      

      <button class="default-btn">Submit</button>

      {{ form_end(registrationForm) }}
    </div>
  </div>
</section>


{% endblock %}
