{% extends './base_front.html.twig' %} {% block title %}edit agent{% endblock %}
{% block body %}

<section class="signup-area ptb-100">
  <div class="container">
    <div class="signup-content">
      <h2>Edit your Account</h2>

      {{ form_start(registrationForm, {'attr': {'class': 'signup-form'}}) }}
      {{ form_errors(registrationForm) }}
      <div class="form-group">
        {{ form_row(registrationForm.nom, {'attr': {'class': 'form-control'}})
        }}
      </div>
      <div class="form-group">
        {{ form_row(registrationForm.prenom, {'attr': {'class': 'form-control'
  


        }}) }}
      </div>
      <div class="form-group">
        {{ form_row(registrationForm.telephone, {'attr': {'class': 'form-control'
  


        }}) }}
      </div>
      <div class="form-group">
        {{ form_row(registrationForm.adresse, {'attr': {'class': 'form-control'
  


        }}) }}
      </div>

      <div class="form-group">
        {{ form_row(registrationForm.gender, {'attr': {'class': 'form-control'
  


        }}) }}
      </div>
      <div class="form-group">
        {{ form_row(registrationForm.image, {'attr': {'class': 'form-control'}})
        }}
      </div>
      <div class="form-group">
        {{ form_row(registrationForm.age, {'attr': {'class': 'form-control'}})
        }}
      </div>

      <button type="submit" class="default-btn">Edit</button>

      {{ form_end(registrationForm) }}
    </div>
  </div>
</section>

{% endblock %}
