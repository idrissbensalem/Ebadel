{% extends 'base.html.twig' %}

{% block title %}Boutique{% endblock %}

{% block body %}
    <h1>Boutique</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ boutique.id }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ boutique.nom }}</td>
            </tr>
            <tr>
                <th>Image</th>
                <td>{{ boutique.image }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ boutique.email }}</td>
            </tr>
            <tr>
                <th>Lien</th>
                <td>{{ boutique.lien }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ boutique.description }}</td>
            </tr>
            <tr>
                <th>Num_telephone</th>
                <td>{{ boutique.numTelephone }}</td>
            </tr>
            <tr>
                <th>Num_fixe</th>
                <td>{{ boutique.numFixe }}</td>
            </tr>
            <tr>
                <th>Gouvernorat</th>
                <td>{{ boutique.gouvernorat }}</td>
            </tr>
            <tr>
                <th>Ville</th>
                <td>{{ boutique.ville }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_boutique_index') }}">back to list</a>

    <a href="{{ path('app_boutique_edit', {'id': boutique.id}) }}">edit</a>

    {{ include('boutique/_delete_form.html.twig') }}
{% endblock %}
